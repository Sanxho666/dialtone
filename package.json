{
  "name": "@dialpad/dialtone-icons",
  "version": "0.6.5-vue3",
  "description": "Dialtone icon library",
  "scripts": {
    "build": "npm run build-icons && npm run build-lib",
    "build-icons": "gulp",
    "build-lib": "vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore",
    "release": "npm install && npm run build && npm publish --tag vue3"
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/svg",
    "dist/icons.json"
  ],
  "type": "module",
  "main": "./dist/dialtone-icons.umd.cjs",
  "module": "./dist/dialtone-icons.js",
  "exports": {
    ".": {
      "import": "./dist/dialtone-icons.js",
      "require": "./dist/dialtone-icons.umd.cjs"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.2",
    "del": "^6.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-json-format": "^2.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-svgmin": "^4.1.0",
    "through2": "^4.0.2",
    "vite": "^4.3.5"
  },
  "peerDependencies": {
    "vue": ">=3.2"
  },
  "repository": "git@github.com:dialpad/dialtone-icons.git"
}
