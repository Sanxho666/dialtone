{
  "name": "@dialpad/dialtone-icons",
  "version": "0.0.9-vue3",
  "description": "Dialtone icon library",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name dialtone-icons src/main.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "release": "npm install && npm run build-lib && npm publish --tag vue3"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": ">=3.2"
  },
  "files": [
    "dist/*.js",
    "dist/svg"
  ],
  "main": "dist/dialtone-icons.common.js",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-mocha": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "peerDependencies": {
    "vue": ">=3.2"
  },
  "repository": "git@github.com:dialpad/dialtone-icons.git"
}
