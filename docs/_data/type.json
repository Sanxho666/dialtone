  "whitespace": [
    {
      "class": "normal",
      "description": "White space sequences are collapsed and newline characters in the source are treated like white space. Lines are broken as needed to fill boxes."
    },
    {
      "class": "nowrap",
      "description": "White space sequences are collapsed like normal, but line breaks are not honored keeping text from wrapping."
    },
    {
      "class": "pre",
      "description": "White space sequences are preserved from the source. Lines are only broken at new line characters and <br> elements."
    },
    {
      "class": "pre-line",
      "description": "White space sequences are collapsed. Lines are broken at new line characters, <br> elements, or as needed to fill boxes."
    },
    {
      "class": "pre-wrap",
      "description": "White space sequences are preserved. Lines are broken at new line characters, <br> elements, or as needed to fill boxes."
    },
    {
      "class": "break-spaces",
      "description": "Acts like pre-wrap except that any sequence of preserved white space always take up space, a line breaking opportunity exists after every preserved white space character, and preserved spaces take up space and do not hang which affects the box's intrinisic sizez (min-content and max-content sizes)."
    },
    {
      "class": "unset",
      "description": "Resets to white-space property to it's default initial value."
    }
  ],
  "align": "left, right, center, justify, unset",
  "word-break": [
    {
      "class": "normal",
      "description": "Uses the default line break rule."
    },
    {
      "class": "break-all",
      "description": "Word breaks are inserted between any two characters (excluding Chinese, Japanese, or Korean text) to prevent text overflowing."
    },
    {
      "class": "keep-all",
      "description": "Word breaks aren't usd for Chinese, Japanese, or Korean (CJK) text. Non-CJK text behavior is set to normal."
    },
    {
      "class": "unset",
      "description": "Resets to the initial, default line break rule."
    }
  ],
  "overflow-wrap": [
    {
      "class": "normal",
      "description": "Lines will only break at normal break points such as a space between two words."
    },
    {
      "class": "anywhere",
      "description": "To keep copy from overflowing its content box, line breaks are inserted at any point in a line. No hyphenation character is inserted at the break point."
    },
    {
      "class": "break-word",
      "description": "The same as the anywhere value, except soft wrap opportunities are not considered when calculating min-content intrinsic sizes."
    },
    {
      "class": "unset",
      "description": "Resets to the initial, default line break rule."
    }
  ],
